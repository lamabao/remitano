<div class="col-lg-3">
</div>
<div class="col-lg-6">
  <form action="/user/addnewuser" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
   
      <h1>Register</h1>
      <p>Please fill in this form to create an account.</p>
      <hr>
      <div class="col-lg-12">
          <%=raw(flash[:quicknotice])%>
          <div class="form-group">
            <label for="email">Email address:</label>
            <input type="text" class="form-control" placeholder="Enter Email" id="email" name="username" autocomplete="off">
        </div>
         <div class="form-group">
          <label for="pwd">Password:</label>
          <input type="password" class="form-control" placeholder="Enter Password" autocomplete="off" name="password">
        </div>
         <div class="form-group">
         <button type="submit" class="btn btn-default">Register</button>
        </div>
      </div>
</form>
</div>
<div class="col-lg-3">
</div>

<div class="clearfix"></div>
